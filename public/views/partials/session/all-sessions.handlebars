<div class="row mt-3">
    <h2 class="text-light armyfont ml-3 shadowed">Sessions View:</h2>
</div>

<div>
    <table class="text-light">
        <th>Used Date</th>
        <th>First name</th>
        <th>Last name</th>
        <th>Schedule Date</th>
        {{#each session}}
        <tr>
        <td>{{executed_date}}</td>
        <td>{{Client.first_name}}</td>
        <td>{{Client.last_name}}</td>
        <td>{{schedule_date}}</td>
        <td><button class="edit-session" data-id={{id}}>Edit session</button></td>
        </tr>
        {{/each}}
    </table>
    
</div>

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/flatpickr/dist/flatpickr.min.css">
<script src="https://cdn.jsdelivr.net/npm/flatpickr"></script>
<script src="/static/js/all-sessions.js"></script>